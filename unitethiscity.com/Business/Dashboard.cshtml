                            <div style="margin-top:5px;font-size:12px; font-weight:normal;">@(siteBusinessInfo.BusRepPhone)</div>
                            <div style="margin-top:5px;font-size:10px;font-weight:normal;"><a href="mailto:" + @siteBusinessInfo.BusRepEMail +"\">@siteBusinessInfo.BusRepEMail</a></div>
                        </div>
                    </div>
                </div>
                <div style="float:left; width:310px;margin-right:10px;height:80px;">
                    <div style="text-align:center;">
                        <div style="background-color:white;margin-top:0px; padding:10px; border-radius:10px;">
                            <div style="margin-top:5px;font-size:12px; font-weight:bold;">UNITE THIS CITY</div>
                            <div style="margin-top:5px;font-size:12px; font-weight:normal;">@(SiteSettings.GetValue("SupportAddress"))</div>
                            <div style="margin-top:5px;font-size:12px; font-weight:normal;">@(SiteSettings.GetValue("SupportPhone"))</div>
                            <div style="margin-top:5px;font-size:10px;font-weight:normal;"><a href="mailto:" + @SiteSettings.GetValue("SupportEMail") +"\">@SiteSettings.GetValue("SupportEMail")</a></div>
                        </div>
                    </div>
                </div>
            </div>
		</div>
		<br style="clear:left;" />
	</div>